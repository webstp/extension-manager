{
  "name": "extension-manager",
  "displayName": "Extension Manager",
  "description": "Manager your extensions",
  "version": "0.0.2",
  "publisher": "webstp",
  "repository": {
    "type": "github",
    "url": "https://github.com/webstp/extension-manager"
  },
  "engines": {
    "vscode": "^0.10.6"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.installMissing",
    "onCommand:extension.test"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.installMissing",
        "title": "Extension Manager: Install Missing Extensions"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Extension Manager Configuration",
      "properties": {
        "extension-manager.extensions": {
          "type": "array",
          "default": [
            "alefragnani.Bookmarks"
          ],
          "description": "List of extension you wish to install in the form of (publisher).(extensionName)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "http-proxy-agent": "^1.0.0",
    "https-proxy-agent": "^1.0.0",
    "lodash": "^4.5.1",
    "semver": "^5.1.0",
    "typescript": "^1.7.5",
    "vscode": "^0.11.0"
  }
}
